{
  "branches": ["main"],
  "plugins": [
    {
      "name": "commit-analyzer",
      "options": {
        "preset": "conventionalcommits"
      }
    },
    {
      "name": "changelog-generator",
      "options": {
        "preset": "conventionalcommits"
      }
    },
    {
      "name": "files-updater",
      "options": {
        "files": [
          {
            "path": "Cargo.toml",
            "updater": "cargo"
          }
        ]
      }
    }
  ]
}
